        <servlet-name>Proveedor</servlet-name>
        <servlet-class>controlador.ServletProveedor</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Actividades</servlet-name>
        <servlet-class>controlador.ServletActividades</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Actividad</servlet-name>
        <servlet-class>controlador.ServletActividad</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Paciente</servlet-name>
        <servlet-class>controlador.ServletPaciente</servlet-class>
    </servlet>
