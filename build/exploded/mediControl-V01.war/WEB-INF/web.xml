        <servlet-name>Actividad</servlet-name>
        <url-pattern>/Actividad</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Paciente</servlet-name>
        <url-pattern>/Paciente</url-pattern>
    </servlet-mapping>
</web-app>
